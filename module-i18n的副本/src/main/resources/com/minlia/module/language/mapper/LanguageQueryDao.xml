<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minlia.module.language.v1.entity.LanguageQueryDao">
    <resultMap id="BaseResultMap" type="com.minlia.module.language.domain.Language">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <id column="app_id" jdbcType="VARCHAR" property="appId"/>
        <result column="locale" jdbcType="VARCHAR" property="locale"/>
        <result column="code" jdbcType="VARCHAR" property="code"/>
        <result column="value" jdbcType="VARCHAR" property="value"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,app_id, locale, code, value
    </sql>
    <select id="findByX" resultMap="BaseResultMap">
        select
        t.id,t.app_id,t.locale,t.code,t.value
        from mdl_language t
    </select>
</mapper>